platform :ios, '13.0'
install! 'cocoapods', :warn_for_unused_master_specs_repo => false

target 'HelloFresh' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for HelloFresh

   pod 'Kingfisher'
   pod 'Swinject'
   pod 'SnapKit'

end

target 'HelloFreshTests' do
  inherit! :search_paths
end
